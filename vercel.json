{
  "version": 2,
  
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  },
  "env": {
    "PYTHONPATH": "/var/task:/var/task/src:/var/task/backend"
  },
  "regions": ["iad1"]
}
